<div class="blog-index">
  <div class="blog-title">
    <h2 class="header">Jack Schuss - Musings</h2>
  </div>



  <div class="posts-div">
    <ul class="post-list">
      <% @posts.each do |post| %>
        <%= link_to "#{post.title}", post_path(post), class: "post-title" %>
      <% end %>
    </ul>
  </div>
</div>
